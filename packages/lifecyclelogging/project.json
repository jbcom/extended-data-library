{
  "name": "lifecyclelogging",
  "projectType": "library",
  "sourceRoot": "packages/lifecyclelogging/src",
  "tags": ["scope:python", "type:lib"],
  "implicitDependencies": ["extended-data-types"],
  "targets": {
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "uv run pytest packages/lifecyclelogging/tests/ -q",
        "cwd": "{workspaceRoot}"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "uvx ruff check packages/lifecyclelogging/src/",
        "cwd": "{workspaceRoot}"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "uv build --package lifecyclelogging",
        "cwd": "{workspaceRoot}"
      },
      "dependsOn": ["^build"]
    }
  }
}
