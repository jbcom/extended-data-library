{
  "name": "vendor-connectors",
  "projectType": "library",
  "sourceRoot": "packages/vendor-connectors/src",
  "tags": ["scope:python", "type:lib"],
  "implicitDependencies": [
    "extended-data-types",
    "lifecyclelogging",
    "directed-inputs-class"
  ],
  "targets": {
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "uv run pytest packages/vendor-connectors/tests/ -q",
        "cwd": "{workspaceRoot}"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "uvx ruff check packages/vendor-connectors/src/",
        "cwd": "{workspaceRoot}"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "uv build --package vendor-connectors",
        "cwd": "{workspaceRoot}"
      },
      "dependsOn": ["^build"]
    }
  }
}
